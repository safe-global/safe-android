        <argument
            android:name="chain"
            app:argType="io.gnosis.data.models.Chain" />

        <argument
            android:name="selectedAsset"
            app:argType="io.gnosis.safe.ui.assets.coins.CoinsViewData" />

        <argument
            android:name="fromAddress"
            app:argType="string" />

        <argument
            android:name="toAddress"
            app:argType="string" />

        <argument
            android:name="amount"
            app:argType="string" />

        <action
            android:id="@+id/action_sendAssetReviewFragment_to_editAdvancedParamsFragment"
            app:destination="@id/editAdvancedParamsFragment" />

    </fragment>

    <fragment
        android:id="@+id/editAdvancedParamsFragment"
        android:name="io.gnosis.safe.ui.safe.send_funds.EditAdvancedParamsFragment"
        android:label="EditAdavancedParamsFragment"
        tools:layout="@layout/fragment_edit_advanced_params">

        <argument
            android:name="chain"
            app:argType="io.gnosis.data.models.Chain" />

        <argument
            android:name="safeNonce"
            app:argType="string" />

        <argument
            android:name="minSafeNonce"
            app:argType="string" />

        <argument
            android:name="safeTxGas"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="proposedSafeTxGas"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

    </fragment>

    <fragment
        android:id="@+id/successFragment"
        android:name="io.gnosis.safe.ui.safe.send_funds.SuccessFragment"
        android:label="SuccessFragment"
