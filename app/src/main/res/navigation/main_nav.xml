        <argument
            android:name="chain"
            app:argType="io.gnosis.data.models.Chain" />

        <argument
            android:name="txId"
            app:argType="string" />

        <argument
            android:name="amount"
            app:argType="string" />

        <argument
            android:name="token"
            app:argType="string" />

    </fragment>

