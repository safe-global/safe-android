        <argument
            android:name="ownerType"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="ownerAddress"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

    </fragment>

    <fragment
        android:id="@+id/repeatPasscodeFragment"
        android:name="io.gnosis.safe.ui.settings.app.passcode.RepeatPasscodeFragment"
