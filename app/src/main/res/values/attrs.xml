<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SettingItem">
        <attr name="setting_openable" format="boolean" />
        <attr name="setting_has_switch" format="boolean" />
        <attr name="setting_checked" format="boolean" />
        <attr name="setting_name" format="string" />
        <attr name="setting_description" format="string" />
        <attr name="setting_value" format="string" />
        <attr name="setting_image" format="reference" />
    </declare-styleable>

    <declare-styleable name="EndpointItem">
        <attr name="endpoint_name" format="string" />
        <attr name="endpoint_value" format="string" />
    </declare-styleable>

    <declare-styleable name="LabeledValueItem">
        <attr name="item_label" format="string" />
    </declare-styleable>

    <declare-styleable name="AddressItem">
        <attr name="show_address_separator" format="boolean" />
    </declare-styleable>

    <declare-styleable name="NamedAddressItem">
        <attr name="show_named_address_separator" format="boolean" />
    </declare-styleable>

    <declare-styleable name="InfoItem">
        <attr name="info_icon" format="reference" />
        <attr name="info_title" format="string" />
        <attr name="info_text" format="string" />
    </declare-styleable>

    <declare-styleable name="StepIndicator">
        <attr name="step_icon" format="reference" />
        <attr name="step_text" format="string" />
        <attr name="step_active" format="boolean" />
        <attr name="step_connector_margin" format="dimension" />
    </declare-styleable>

    <declare-styleable name="SeedItem">
        <attr name="seed_word" format="string" />
        <attr name="seed_number" format="integer" />
    </declare-styleable>

    <declare-styleable name="WhatsNewItem">
        <attr name="whatsnew_icon" format="reference" />
        <attr name="whatsnew_title" format="string" />
        <attr name="whatsnew_description" format="string" />
    </declare-styleable>

    <declare-styleable name="InfoLabelView">
        <attr name="info_label_icon" format="reference" />
        <attr name="info_label_text" format="string" />
    </declare-styleable>

</resources>
